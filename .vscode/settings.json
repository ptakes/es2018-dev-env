{
  "coverage-gutters.coverageFileNames": ["coverage/lcov.info"],
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.mouseWheelZoom": true,
  "editor.tabSize": 2,
  "eslint.enable": true,
  "files.autoSave": "off",
  "files.exclude": {
    ".nyc_output/": true,
    "node_modules/": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.nyc_output/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules/*/**": true
  },
  "html.format.wrapLineLength": 180,
  "mochaExplorer.env": { "NODE_ENV": "test" },
  "mochaExplorer.files": "src/**/*.test.js",
  "mochaExplorer.optsFile": "mocha.opts",
  "mochaExplorer.mochaPath": "node_modules/mocha",
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 180,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "sassFormat.indent": 2,
  "sassFormat.useSingleQuotes": true,
  "sasslint.enable": true,
  "tslint.enable": false,
  "window.zoomLevel": 0
}
