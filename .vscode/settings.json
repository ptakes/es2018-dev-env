{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.fixAll.style": true
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.mouseWheelZoom": true,
  "editor.tabSize": 2,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.enable": true,
  "files.autoSave": "off",
  "files.exclude": {
    ".chrome/": true,
    ".git/": true,
    ".nyc_output/": true,
    ".tmp/": true,
    ".vs/": true,
    "node_modules/": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/$tf/**": true,
    "**/.chrome/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.tmp/*/**": true,
    "**/.nyc_output/*/**": true,
    "**/.vs/*/**": true,
    "**/coverage/*/**": true,
    "**/dist/*/**": true,
    "**/node_modules/*/**": true
  },
  "html.format.wrapLineLength": 180,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "mochaExplorer.files": "src/**/*.test.js",
  "mochaExplorer.require": "@babel/register",
  "sasslint.enable": true,
  "window.zoomLevel": 0,
  "vs-code-prettier-eslint.eslintFileName": ".eslintrc",
  "vs-code-prettier-eslint.prettierFileName": ".prettierrc"
}
