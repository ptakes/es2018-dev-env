{
  "private": true,
  "name": "es2018-dev-env",
  "version": "1.0.0",
  "description": "Default ECMAScript 2018 Development Environment.",
  "author": "Peter Takes",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "postbuild": "babel-node build/buildServer.js",
    "clone": "babel-node build/cloneProject.js",
    "start": "cross-env NODE_ENV=development nodemon --watch ./*.js* --watch build --ext js --exec \"webpack-dev-server --mode development\"",
    "test": "cross-env NODE_ENV=test nyc mocha --opts ./mocha.opts"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bootstrap": "^4.2.1",
